{
  "base_keymap": "VSCode",
  "theme": "Ros√© Pine Dawn",
  "buffer_font_family": "Hack",
  "buffer_font_size": 15,
  "tab_size": 2,
  "format_on_save": "on",
  "git": {
    "git_gutter": "tracked_files"
  },
  "language_overrides": {
    "JavaScript": {
      "format_on_save": {
        "external": {
          "command": "npx",
          "arguments": [
            "prettier",
            "--stdin-filepath",
            "{buffer_path}"
          ]
        }
      }
    },
    "TypeScript": {
      "format_on_save": {
        "external": {
          "command": "npx",
          "arguments": [
            "prettier",
            "--stdin-filepath",
            "{buffer_path}"
          ]
        }
      }
    }
  }
}
